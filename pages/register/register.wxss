.flexCenter {
  display: flex;
  justify-content: center;
  align-items: center;
}
.mask {
  display: none;
  height: 100vh;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 99;
  justify-content: center;
  align-items: center;
}
.mask.show {
  display: flex;
}
.clipOneLine {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.container .logo {
  width: 200rpx;
  display: block;
  margin: 0 auto;
  margin-top: 40rpx;
}
.container .text {
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  margin-top: 40rpx;
}
.container .tabs {
  display: flex;
  width: 600rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  position: relative;
}
.container .tabs.item1 .tabItem.item1 {
  color: #333333;
  font-weight: bold;
}
.container .tabs.item1::before {
  transform: translateX(0);
}
.container .tabs.item2 .tabItem.item2 {
  color: #333333;
  font-weight: bold;
}
.container .tabs.item2::before {
  transform: translateX(154rpx);
}
.container .tabs .tabItem {
  width: 120rpx;
  color: #666666;
  font-size: 28rpx;
  text-align: center;
  padding-bottom: 24rpx;
}
.container .tabs .tabItem:last-child {
  margin-left: 34rpx;
}
.container .tabs::before {
  content: "";
  display: block;
  width: 120rpx;
  border-bottom: 4rpx solid #21439B;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 1;
  transition: transform 0.2s;
}
.container .tabs::after {
  content: "";
  display: block;
  width: 600rpx;
  border-bottom: 1px solid #E7E5F2;
  position: absolute;
  bottom: 0;
  left: 0;
  border-radius: 1rpx;
}
.container .formContent {
  width: 600rpx;
  overflow: hidden;
  margin: 0 auto;
}
.container .formContent .input {
  position: relative;
  height: 70rpx;
  border: 1px solid #D7D7D7;
  border-radius: 7rpx;
  margin-top: 30rpx;
}
.container .formContent .input input {
  box-sizing: border-box;
  border: none;
  height: 65rpx;
  outline: none;
  background-color: transparent;
  padding-left: 73rpx;
}
.container .formContent .input input:-webkit-input-placeholder {
  font-size: 28rpx;
  color: #999999;
}
.container .formContent .input .icon {
  width: 22rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.container .formContent .input .areaIcon {
  width: 15rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.container .formContent .input.phone {
  display: flex;
  padding-left: 76rpx;
  align-items: center;
}
.container .formContent .input.phone input {
  padding-left: 20rpx;
}
.container .formContent .input.name input {
  padding-right: 150rpx;
}
.container .formContent .input.name #sex {
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 28rpx;
  color: #999999;
  display: flex;
  align-items: center;
  z-index: 99;
}
.container .formContent .input.name #sex .sexIcon {
  width: 20rpx;
  margin-left: 8rpx;
}
.container .formContent .input.name .options {
  padding: 10rpx 0;
  position: absolute;
  right: 20rpx;
  bottom: 0;
  transform: translateY(100%);
  width: 110rpx;
  z-index: 99;
  background-color: #fff;
  box-shadow: 0px 6rpx 17rpx 1rpx rgba(153, 153, 153, 0.2);
  display: none;
}
.container .formContent .input.name .options.show {
  display: block;
}
.container .formContent .input.name .options .item {
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
}
.container .formContent .input.name::after {
  content: "";
  display: block;
  height: 23rpx;
  border-right: 1px solid #c4c4c4;
  position: absolute;
  right: 130rpx;
  top: 50%;
  transform: translateY(-50%);
}
.container .formContent .input.code .getCodeBtn {
  width: 140rpx;
  height: 44rpx;
  color: #fff;
  font-size: 22rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #21439B;
  border-radius: 22rpx;
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 9;
}
.container .formContent .license {
  margin-top: 30rpx;
}
.container .formContent .license .licenseTitle {
  font-size: 28rpx;
  margin-bottom: 20rpx;
}
.container .formContent .agreement {
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  color: #999999;
  font-size: 26rpx;
  position: relative;
}
.container .formContent .agreement .highlight {
  color: #21439B;
}
.container .formContent .ronBtn {
  width: 600rpx;
  height: 78rpx;
  background-color: #21439B;
  font-size: 36rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 50rpx;
  border-radius: 39rpx;
}
.tip {
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 30rpx;
  font-size: 24rpx;
  color: #999999;
}
.tip .toLogin {
  color: #21439B;
}
