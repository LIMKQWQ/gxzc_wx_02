.flexCenter {
  display: flex;
  justify-content: center;
  align-items: center;
}

.mask {
  display: none;
  height: 100vh;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 99;
  justify-content: center;
  align-items: center;
}

.mask.show {
  display: flex;
}

.clipOneLine {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.container {
  height: 100vh;
}

.container .indexSwiper {
  width: 100vw;
  height: 100vh;
  overflow: hidden;
  position: relative;
  z-index: 98;
}

.container .indexSwiper .mainContent {
  width: 100%;
  position: absolute;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 9;
}

.container .indexSwiper .mainContent .title {
  font-size: 54rpx;
  font-weight: bold;
  color: #fff;
  text-align: center;
}

.container .indexSwiper .mainContent .text {
  margin-top: 44rpx;
  text-align: center;
  font-size: 32rpx;
  height: 48rpx;
  display: flex;
  justify-content: center;
}

.container .indexSwiper .mainContent .text view {
  color: #fff;
  position: relative;
}

.container .indexSwiper .mainContent .text view::after {
  content: "";
  display: block;
  position: absolute;
  width: 100%;
  bottom: -8rpx;
  border-top: 1px solid #fff;
}

.container .indexSwiper .swiper {
  width: 100vw;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
}

.container .indexSwiper .swiper .swiperItem {
  height: 100vh;
}

.container .indexSwiper .swiper .swiperItem image {
  width: 100vw;
  height: 100vh;
}

.container .indexSwiper .btns {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  justify-content: center;
  z-index: 10;
}

.container .indexSwiper .btns .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 50rpx;
  background: linear-gradient(0deg, #92F1F0, #1FCFCF, #1BBAB9);
  border-radius: 25rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 26rpx;
}

.container .indexSwiper .btns .btn.findSource {
  width: 162rpx;
}

.container .indexSwiper .btns .btn.toBeSource {
  width: 182rpx;
}

.container .indexSwiper .btns .btn:last-child {
  margin-left: 34rpx;
}

.container .indexSwiper .btns .btn image {
  width: 23rpx;
  margin-left: 8rpx;
}

.container .indexSwiper .changeBtn {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  width: 16rpx;
  height: 27rpx;
  padding: 20rpx;
}

.container .indexSwiper .changeBtn.pre {
  left: 10rpx;
}

.container .indexSwiper .changeBtn.next {
  right: 10rpx;
}

.container .notices text {
  margin: 0 50rpx;
  color: #333333;
}

.container .census {
  margin-top: 60rpx;
  min-width: 100%;
  position: relative;
  width: 100%;
}

.container .census .censusContainer {
  width: 530rpx;
  margin: 0 auto;
}

.container .census .censusContainer .censusRow {
  display: flex;
  justify-content: space-between;
  margin-top: 22rpx;
  position: relative;
}

.container .census .censusContainer .censusRow:last-child {
  margin-top: 50rpx;
}

.container .census .censusContainer .censusRow::after {
  content: "";
  display: block;
  height: 60rpx;
  border-right: 1px solid #d4d4d4;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%) scaleX(0.5);
}

.container .census .censusContainer .censusRow .censusItem {
  width: 350rpx;
  text-align: center;
}

.container .census .censusContainer .censusRow .censusItem .censusData {
  color: #666666;
}

.container .census .censusContainer .censusRow .censusItem .censusData .num {
  font-size: 58rpx;
  font-weight: bold;
  color: #21439B;
  vertical-align: top;
}

.container .census .censusContainer .censusRow .censusItem .censusTitle {
  font-size: 22rpx;
  color: #666666;
}

.container .hotList {
  margin-top: 65rpx;
}

.container .hotList .top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}

.container .hotList .top .title {
  font-size: 34rpx;
  font-weight: 500;
  font-weight: bold;
}


.container .hotList .top .more {
  display: flex;
  align-items: center;
}

.container .hotList .top .more view {
  font-size: 22px;
  color: #21439B;
  margin-right: 6rpx;
}

.container .hotList .top .more image {
  width: 22rpx;
  height: 22rpx;
  margin-left: 4rpx;
}

.container .hotList .swiper-container {
  box-sizing: border-box;
  height: 980rpx;
}

.container .hotList .swiper-container .wx-swiper-dot {
  width: 25rpx;
  height: 4rpx;
  background-color: #E0E0E0;
  border-radius: 0;
}

.container .hotList .swiper-container .wx-swiper-dot.wx-swiper-dot-active {
  background: #21439B;
  border-radius: 0;
}

.container .hotList .swiper-container .list {
  margin-top: 39rpx;
  height: 900rpx;
}

.container .hotList .swiper-container .list .row {
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
}

.container .hotList .swiper-container .list .row:last-child {
  margin-top: 30rpx;
}

.container .hotList .swiper-container .list .row .listItem {
  width: 335rpx;
  padding-bottom: 20rpx;
  box-shadow: 0rpx 6rpx 17rpx 1rpx rgba(153, 153, 153, 0.2);
}

.container .hotList .swiper-container .list .row .listItem .thumbnail image {
  width: 100%;
  height: 229rpx;
  object-fit: cover;
}

.container .hotList .swiper-container .list .row .listItem .title {
  padding: 15rpx;
  font-weight: bold;
  font-size: 28rpx;
}

.container .hotList .swiper-container .list .row .listItem .info {
  display: flex;
  justify-content: space-between;
  padding: 0 10rpx;
  font-size: 22rpx;
  color: #666666;
  overflow: hidden;
}

.container .hotList .swiper-container .list .row .listItem .info .stock {
  width: 45%;
  overflow-x: hidden;
  word-break: keep-all;
}

.container .hotList .swiper-container .list .row .listItem .info .distance {
  width: 55%;
}

.container .hotList .swiper-container .list .row .listItem .info .distance image {
  width: 20rpx;
}

.container .hotList .swiper-container .list .row .listItem .toKf {
  margin-top: 23rpx;
}

.container .hotList .swiper-container .list .row .listItem .toKf .btn {
  display: block;
  width: 144rpx;
  height: 44rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  background-color: #019E97;
  border-radius: 22rpx;
  color: #ffffff;
}

.container .hotList .swiper-container .list .row .listItem .toKf .btn image {
  width: 18rpx;
  margin-left: 6rpx;
}

.container .sharingKnowledge {
  margin-top: 24rpx;
  padding: 0 20rpx;
  padding-bottom: 26rpx;
}

.container .sharingKnowledge .top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 !important;
}

.container .sharingKnowledge .top .title {
  font-size: 34rpx;
  font-weight: 500;
  font-weight: bold;
}

.container .sharingKnowledge .top .more {
  display: flex;
  align-items: center;
}

.container .sharingKnowledge .top .more view {
  font-size: 22rpx;
  color: #21439B;
  margin-right: 6rpx;
}

.container .sharingKnowledge .top .more image {
  width: 22rpx;
  margin-left: 4rpx;
}

.container .sharingKnowledge .list {
  margin-top: 39rpx;
}

.container .sharingKnowledge .list .listItem {
  margin-bottom: 40rpx;
  height: 130rpx;
  box-shadow: 0px 6rpx 17rpx 1rpx rgba(153, 153, 153, 0.2);
  border-radius: 10rpx;
  padding: 0 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: relative;
}

.container .sharingKnowledge .list .listItem:last-child {
  margin-bottom: 0;
}

.container .sharingKnowledge .list .listItem .title {
  font-size: 28rpx;
  font-weight: bold;
  width: 480rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.container .sharingKnowledge .list .listItem .time {
  margin-top: 22rpx;
  color: #666666;
}

.container .sharingKnowledge .list .listItem .toInfo {
  position: absolute;
  right: 30rpx;
  top: 50%;
  width: 144rpx;
  height: 44rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #21439B;
  transform: translateY(-50%);
  font-size: 22rpx;
  color: #21439B;
}

.top .title {
  padding-left: 20rpx;
}

.container .sortList {
  margin-top: 65rpx;
}

.top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}

.top .title {
  font-size: 34rpx;
  font-weight: 500;
  font-weight: bold;
  position: relative;
}

.top .title::before{
  content: "";
  display: block;
  width: 6rpx;
  height: 32rpx;
  background-color: #21439B;
  position: absolute;
  left: 0;
  top: 50%;
  border-radius: 6rpx;
  transform: translateY(-50%);
}

.top .more {
  display: flex;
  align-items: center;
}

.top .more view {
  font-size: 22px;
  color: #21439B;
  margin-right: 6rpx;
}

.top .more image {
  width: 22rpx;
  height: 22rpx;
  margin-left: 4rpx;
}

.sortList .sortIcon {
  width: 80rpx;
  height: 80rpx;
  border: 1rpx dashed #ccc;
  position: relative;
  margin: auto;
}

.sortList .sortIcon image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}

.sortList .list {
  display: grid;
  padding: 0 40rpx;
  grid-template-columns: 1fr 1fr 1fr 1fr;
  grid-gap: 50rpx;
  margin-top: 39rpx;
}

.sortList .list .item .sortName {
  text-align: center;
  margin-top: 12rpx;
  font-size: 24rpx;
  color: #333;
}

.newsList {
  flex-grow: 99;
  margin: 39rpx auto;
}
.newsList .list{
  padding: 0 20rpx;
}
.newsList .newsItem {
  display: flex;
  height: 145rpx;
  position: relative;
  margin-bottom: 80rpx;
}

.newsList .newsItem:first-child {
  padding-top: 40rpx;
}

.newsList .newsItem::after {
  content: "";
  display: block;
  position: absolute;
  bottom: -40rpx;
  width: 100%;
  border-bottom: 1rpx solid #E7E5F2;
}

.newsList .newsItem .leftInfo {
  width: 433rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.newsList .newsItem .leftInfo .title {
  font-size: 30rpx;
  color: #333333;
  line-height: 46rpx;
  height: 95rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.newsList .newsItem .leftInfo .info {
  display: flex;
  color: #666666;
}

.newsList .newsItem .leftInfo .info .views {
  margin-left: 33rpx;
}

.newsList .newsItem .leftInfo .info .views image {
  width: 24rpx;
}

.newsList .newsItem .rightInfo {
  margin-left: 48rpx;
}

.newsList .newsItem .rightInfo .pic image {
  width: 225rpx;
  height: 145rpx;
}