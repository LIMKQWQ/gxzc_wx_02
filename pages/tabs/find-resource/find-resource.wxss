.flexCenter {
  display: flex;
  justify-content: center;
  align-items: center;
}

.mask {
  display: none;
  height: 100vh;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 99;
  justify-content: center;
  align-items: center;
}

.mask.show {
  display: flex;
}

.clipOneLine {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.lodingMoreIcon {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  height: 100rpx;
  width: 100%;
}

.container .banner {
  height: 280rpx;
}

.container .banner image {
  height: 280rpx;
  width: 100%;
}

