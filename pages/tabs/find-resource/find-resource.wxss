.container .newsInfo .infoSwiper {
  margin-top: 29rpx;
  height: 312rpx;
}

.container .newsInfo .infoSwiper .wx-swiper-dot {
  width: 25rpx;
  height: 4rpx;
  background-color: #E0E0E0;
  border-radius: 0;
}

.container .newsInfo .infoSwiper .wx-swiper-dot.wx-swiper-dot-active {
  background: #21439B;
  border-radius: 0;
}

.container .newsInfo .infoSwiper .swiperItem {
  width: 342rpx;
  height: 262rpx;
}

.container .newsInfo .infoSwiper .swiperItem .list {
  display: flex;
  justify-content: space-between;
}

.container .newsInfo .infoSwiper .swiperItem .list .newsItem {
  width: 342rpx;
  height: 262rpx;
  position: relative;
  box-shadow: 0rpx 6rpx 17rpx 1rpx rgba(153, 153, 153, 0.2)
}

.container .newsInfo .infoSwiper .swiperItem .list .newsItem .content {
  width: 342rpx;
  max-height: 175rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 2;
  color: #000;
  padding: 0 20rpx;
  padding-top: 10rpx;
