  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.container .newsInfo .infoSwiper .swiperItem .list .newsItem .content .time {
  margin-top: 15rpx;
  font-size: 20rpx;
  margin-bottom: 10rpx;
  color: #999;
}

.container .newsInfo .infoSwiper .swiperItem .list .newsItem image {
  position: absolute;
  top: 0;
  left: 0;
  width: 342rpx;
  height: 262rpx;
}

.container .resourcontent {
  width: 690rpx;
  margin: 0 auto;
  margin-top: 43rpx;
}

.container .resourcontent .resoutop .resoutopRow {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 30rpx;
}

.container .resourcontent .resoutop .resoutopRow .selectx {
  display: flex;
  align-items: center;
  position: relative;
  width: 200rpx;
}

.container .resourcontent .resoutop .resoutopRow .selectx:last-child {
  margin-right: 0;
}

.container .resourcontent .resoutop .resoutopRow .selectx .dropdown {
  max-width: 200rpx;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.container .resourcontent .resoutop .resoutopRow .selectx image {
  margin-left: 20rpx;
  width: 17rpx;
}

.container .resourcontent .resoutop .resoutopRow .filterBtn {
  width: 200rpx;
}

.container .resourcontent .resoutop .resoutopRow .filterBtn .seach {
  width: 100rpx;
  height: 23rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #21439B;
  min-width: 100rpx;
  text-align: center;
  height: 38rpx;
  border: 1px solid #21439B;
  border-radius: 22rpx;
}

.container .resourcontent .inputseach {
  position: relative;
  height: 66rpx;
  margin-top: 40rpx;
  width: 690rpx;
}

.container .resourcontent .inputseach image {
  position: absolute;
  top: 0;
  left: 20rpx;
  bottom: 0;
  margin: auto;
  width: 27rpx;
}

.container .resourcontent .inputseach .searchBtn {
  width: 150rpx;
  height: 68rpx;
  position: absolute;
  /* top: 50%; */
  right: 0;
  top: 0;
  /* transform: translateY(-49%); */
  background-color: #21439B;
  text-align: center;
  line-height: 68rpx;
  color: #fff;
  border-radius: 33rpx;
  z-index: 9;
  font-size: 28rpx;
}

.container .resourcontent .inputseach input {
  font-size: 26rpx;
  font-weight: 400;
  color: #999999;
  padding-left: 60rpx;
  padding-right: 130rpx;
  height: 66rpx;
  border: 1px solid #21439B;
  border-radius: 33rpx;
}

.container .tip {
  padding-left: 30rpx;
  margin: 30rpx 0;
  color: #666666;
}

.container .resourceList {
  margin-top: 40rpx;
  padding: 0 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.container .resourceList .resourceItem {
  width: 335rpx;
  padding-bottom: 84rpx;
  margin-bottom: 30rpx;
  position: relative;
  box-shadow: 0rpx 6rpx 17rpx 1rpx rgba(153, 153, 153, 0.2);
}

.container .resourceList .resourceItem .pic {
  position: relative;
  width: 335rpx;
  height: 260rpx;
}

.container .resourceList .resourceItem .pic .picIcon {
  width: 335rpx;
  height: 260rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.container .resourceList .resourceItem .pic .distance {
  height: 44rpx;
  background-color: #fff;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 1;
  padding: 0 10rpx;
  display: flex;
  align-items: center;
  color: #666;
  font-size: 22rpx;
}

.container .resourceList .resourceItem .pic .distance .distanceIcon {
  width: 20rpx;
  height: 20rpx;
  margin-right: 4rpx;
}

.container .resourceList .resourceItem .info {
  padding-top: 20rpx;
  padding-left: 10rpx;
  padding-right: 26rpx;
}

.container .resourceList .resourceItem .info .infoTitle {
  font-size: 28rpx;
  font-weight: bold;
  width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.container .resourceList .resourceItem .info .tags {
  font-size: 22rpx;
  color: #666;
  display: flex;
  margin-top: 21rpx;
}

.container .resourceList .resourceItem .info .pak {
  font-size: 22rpx;
  color: #666;
  /* display: flex; */
  /* justify-content: space-between; */
}

.container .resourceList .resourceItem .toKf {
  width: 169rpx;
  height: 44rpx;
  background-color: #019E97;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 22rpx;
  border-radius: 22rpx;
  margin: 0 auto;
  margin-top: 19rpx;
  position: absolute;
  bottom: 20rpx;
  left: 50%;
  transform: translateX(-50%);
}

.container .resourceList .resourceItem .toKf .toKfIcon {
  width: 18rpx;
  height: 18rpx;
  margin-left: 7rpx;
}

.container .resourceList .resourceItem .info .pak>view {
  display: flex;
  margin-top: 15rpx;
  align-items: center;
}

.container .resourceList .resourceItem .info .pak .pakitembet {
  justify-content: space-between;
}

.container .resourceList .resourceItem .info .pak .pakitembet>view {
  max-width: 49%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.pak .pak_cz {
  color: #21439B;
}

.pak .pak_cs {
  color: #CF1D1D;
}

.container .resourceList .resourceItem .intr {
  font-size: 22rpx;
  margin-top: 21rpx;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.filterBar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  z-index: 98;
}

.filterBar .filterTabs {
  display: flex;
  align-items: center;
}

.filterBar .filterTabs text {
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
  margin-right: 40rpx;
}

.filterBar .registerBtn {
  display: block;
  color: #21439B;
  border: 1rpx solid #21439B;
  border-radius: 4rpx;
  text-align: center;
  height: 48rpx;
  line-height: 48rpx;
  width: 150rpx;
}

.filterBar .filterMenu {
  position: absolute;
  top: 44rpx;
  left: 0;
  width: 100%;
}

.filterBar .filterMenu .filterCont {
  background-color: #fff;
  padding: 0 30rpx;
  box-shadow: 0rpx 10rpx 25rpx 5rpx rgba(153, 153, 153, 0.5);
  width: 100%;
}

.filterBar .filterMenu .filterCont .filterList {
  border-bottom: 1px solid #f0f0f0;
}

.filterBar .filterMenu .filterCont .filterList .filterTit {
  line-height: 66rpx;
  font-weight: bold;
}

.filterBar .filterMenu .filterCont .filterList .filterOption {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}

.filterBar .filterMenu .filterCont .filterList .filterOption text {
  display: block;
  margin-bottom: 20rpx;
  margin-right: 20rpx;
}